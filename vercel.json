{
    "headers": [
    {
    "source": "/(.*)",
    "headers": [
    {
    "key": "Cache-Control",
    "value": "public, max-age=0, s-maxage=3600, must-revalidate"
    }
    ]
    },
    {
    "source": "/(.*).(.*).:ext(js|css|png|jpg|jpeg|gif|svg|woff|woff2)",
    "headers": [
    {
    "key": "Cache-Control",
    "value": "public, max-age=60, s-maxage=3600"
    }
    ]
    }
    ]
    }